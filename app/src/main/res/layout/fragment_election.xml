<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.c4q.passionproject.MVPController.ElectionFragment">

    <!-- TODO: Update blank fragment layout -->
    <EditText
        android:id="@+id/addressinputElection"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:text="1263 Pacific Ave Kansas City KS"/>


    <EditText
        android:id="@+id/electionIdElection"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:inputType="number"
        android:text="2000"/>

    <Button
        android:id="@+id/submitButtonElection"
        android:textSize="25sp"
        android:hint="submit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <android.support.v7.widget.RecyclerView
        android:id="@+id/electionRv"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    
</LinearLayout>
